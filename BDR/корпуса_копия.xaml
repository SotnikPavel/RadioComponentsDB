        <ComboBox x:Name="comboBox_Copy" HorizontalAlignment="Left" Margin="77,201,0,0" VerticalAlignment="Top" Width="186" Height="17"/>
        <TextBox x:Name="textBox_Copy2" HorizontalAlignment="Left" Height="18" Margin="77,239,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="54"/>
        <TextBox x:Name="textBox_Copy3" HorizontalAlignment="Left" Height="18" Margin="142,239,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="54"/>
        <TextBox x:Name="textBox_Copy4" HorizontalAlignment="Left" Height="18" Margin="209,239,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="54" RenderTransformOrigin="1.005,0.557"/>

